{
    "name": "Twig",
    "version": "0.0.1",

    
    "description": "Branch out a little every day",
    "icons": {
        "16": "images/ext_logo.png",
        "32": "images/ext_logo.png",
        "48": "images/ext_logo.png",
        "128": "images/ext_logo.png"
    },

    "permissions": ["storage",
                    "tabs"
                    ],
    "action": {
        "default_icon": {
            "16": "images/ext_logo.png",
            "32": "images/ext_logo.png"
          }
    },
    
    "chrome_url_overrides" : {
        "newtab": "newtab.html"
    },
    "background": {
        "service_workers": "background.js"
       
    },
    "manifest_version": 3
}